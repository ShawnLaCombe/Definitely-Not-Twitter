Liked by:
<% @tweet.likes.each do |like| %>
<br>
  <%= like.user.username %>
<br>
<% end %>
<%= @tweet.likes.count %><br>


<% if current_user.likes?(@tweet) %>
  <%= link_to "Unlike", tweet_likes_path(@tweet), method: :delete, class: "btn btn-sm btn-success" %><br>

<% else %>
  <%= link_to "Like", tweet_likes_path(@tweet), method: :post %>
<% end %>




